{
    "version": 2,
    "buildCommand": "npm run build:prod",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}
